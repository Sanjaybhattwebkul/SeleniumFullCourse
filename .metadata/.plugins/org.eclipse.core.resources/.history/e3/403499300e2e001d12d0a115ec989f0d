import java.util.*;

public class String {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String  name = "Sanjay Bhatt";
		 int count=0;
		
		  //Counts each character except space    
        for(int i = 0; i < name.length(); i++) {    
           
			if(name.charAt(i) != ' ')    
                count++;    
        }    

	}

}
